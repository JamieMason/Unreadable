#!/bin/bash

cwd="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"
cd $cwd/../

clear

echo "Alexa #1 Google"
./unreadable --url "http://google.com" --inspect --output "test-docs/google.min.html"
echo "Alexa #2 Facebook"
./unreadable --url "http://facebook.com" --inspect --output "test-docs/facebook.min.html"
echo "Alexa #3 YouTube"
./unreadable --url "http://youtube.com" --inspect --output "test-docs/youtube.min.html"
echo "Alexa #4 Yahoo!"
./unreadable --url "http://uk.yahoo.com" --inspect --output "test-docs/yahoo.min.html"
echo "Alexa #5 Baidu.com"
./unreadable --url "http://baidu.com" --inspect --output "test-docs/baidu.min.html"
echo "Alexa #6 Wikipedia"
./unreadable --url "http://wikipedia.org" --inspect --output "test-docs/wikipedia.min.html"
echo "Alexa #7 Windows Live"
./unreadable --url "http://live.com" --inspect --output "test-docs/live.min.html"
echo "Alexa #8 QQ.COM"
./unreadable --url "http://qq.com" --inspect --output "test-docs/qq.min.html"
echo "Alexa #9 Twitter"
./unreadable --url "http://twitter.com" --inspect --output "test-docs/twitter.min.html"
echo "Alexa #10 Amazon.com"
./unreadable --url "http://amazon.com" --inspect --output "test-docs/amazon.min.html"
